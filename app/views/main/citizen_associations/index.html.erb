<p id="notice"><%= notice %></p>

<h1>Main Citizen Associations</h1>

<table>
  <thead>
    <tr>
      <th>Citizen association</th>
      <th>Main sub district</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @main_citizen_associations.each do |main_citizen_association| %>
      <tr>
        <td><%= main_citizen_association.citizen_association %></td>
        <td><%= main_citizen_association.main_sub_district %></td>
        <td><%= link_to 'Show', main_citizen_association %></td>
        <td><%= link_to 'Edit', edit_main_citizen_association_path(main_citizen_association) %></td>
        <td><%= link_to 'Destroy', main_citizen_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Main Citizen Association', new_main_citizen_association_path %>
