<% @datas[:cities].each do |data| %>
    <p><%= data["cities"].city %></p>
    <% data["districts"].each do |district|%>
        <p><%= district["district"].district %></p>
        <% district["sub_districts"].each do |sub_district| %>
            <p><%= sub_district["sub_district"]["count_confirm"] %></p>
        <% end %>
    <%end %>
<% end %>
<%= paginate @cities %>