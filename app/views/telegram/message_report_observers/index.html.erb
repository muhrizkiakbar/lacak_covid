<p id="notice"><%= notice %></p>

<h1>Telegram Message Report Observers</h1>

<table>
  <thead>
    <tr>
      <th>Chat ID</th>
      <th>Username telegram</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telegram_message_report_observers.each do |telegram_message_report_observer| %>
      <tr>
        <td><%= telegram_message_report_observer.chat_id %></td>
        <td><%= telegram_message_report_observer.username_telegram %></td>
        <td><%= telegram_message_report_observer.message %></td>
        <td><%= link_to 'Show', telegram_message_report_observer %></td>
        <td><%= link_to 'Edit', edit_telegram_message_report_observer_path(telegram_message_report_observer) %></td>
        <td><%= link_to 'Destroy', telegram_message_report_observer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Telegram Message Report Observer', new_telegram_message_report_observer_path %>
