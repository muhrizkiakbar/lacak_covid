<p id="notice"><%= notice %></p>

<h1>Telegram Message Report Observers</h1>

<table>
  <thead>
    <tr>
      <th>Chat ID</th>
      <th>Username telegram</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telegram_message_report_observers.each do |telegram_message_report_observer| %>
      <tr>
        <td><%= telegram_message_report_observer.chat_id %></td>
        <td><%= telegram_message_report_observer.username_telegram %></td>
        <td><%= telegram_message_report_observer.message %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

