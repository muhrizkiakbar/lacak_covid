          <div class="col-md">
            <span class="h6 surtitle text-muted">Jenis Kontak</span>
            <span class="d-block h3">
              <%= (@lampiran_eleven_close_contact_information.information_expose.type_contact.type_contact.nil? == true) ? @lampiran_eleven_close_contact_information.information_expose.other_type_contact : @lampiran_eleven_close_contact_information.information_expose.type_contact.type_contact %>
            </span>
          </div>
        </div>
        <div class="row">
          <div class="col-md">
            <span class="h6 surtitle text-muted">Tanggal</span>
            <span class="d-block h3">
              <%= @lampiran_eleven_close_contact_information.information_expose.date_contact.strftime("%d/%m/%Y") %>
            </span>
          </div>
        </div>
        <div class="row">
          <div class="col-md">
            <span class="h6 surtitle text-muted">Durasi</span>
            <span class="d-block h3">
              <%= @lampiran_eleven_close_contact_information.information_expose.duration_contact %>
            </span>
          </div>
        </div>
        <div class="row">
          <div class="col-md">
            <span class="h6 surtitle text-muted">Lokasi</span>
            <span class="d-block h3">
              <%= (@lampiran_eleven_close_contact_information.information_expose.set_location.set_location.nil? == true) ? @lampiran_eleven_close_contact_information.information_expose.other_set_location : @lampiran_eleven_close_contact_information.information_expose.set_location.set_location %>
            </span>
          </div>
        </div>
        <% end %>
      </div>
    </div>
    

    <%# 4.1 %>
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col">
            <h3 class="mb-0">4.1. Kontak Erat</h3>
          </div>
        </div>
      </div>
      <div class="card-body">
