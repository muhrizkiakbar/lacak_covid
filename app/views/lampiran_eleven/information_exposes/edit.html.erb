        <div class="card">
            <div class="card-header">
                <h3 class="mb-0">4. Informasi Paparan Edit</h3>
            </div>
            <div class="card-body">
                <%= render 'form', lampiran_eleven_information_expose: @lampiran_eleven_information_expose %>
