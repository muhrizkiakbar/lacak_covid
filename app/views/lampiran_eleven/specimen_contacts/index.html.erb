<p id="notice"><%= notice %></p>

<h1>Lampiran Eleven Specimen Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Type of specimen</th>
      <th>Date of specimen collection</th>
      <th>Laboratory examination results</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <% if !@lampiran_eleven_close_contact_information.specimen_contact.nil? %>
  <tbody>
      <tr>
        <td><%= @lampiran_eleven_close_contact_information.specimen_contact.type_of_specimen %></td>
        <td><%= @lampiran_eleven_close_contact_information.specimen_contact.date_of_specimen_collection %></td>
        <td><%= @lampiran_eleven_close_contact_information.specimen_contact.laboratory_examination_results %></td>
        <td><%= link_to 'Show', lampiran_eleven_close_contact_information_specimen_contact_path(@lampiran_eleven_close_contact_information,@lampiran_eleven_close_contact_information.specimen_contact) %></td>
        <td><%= link_to 'Edit', edit_lampiran_eleven_close_contact_information_specimen_contact_path(@lampiran_eleven_close_contact_information,@lampiran_eleven_close_contact_information.specimen_contact) %></td>
        <td><%= link_to 'Destroy', lampiran_eleven_close_contact_information_specimen_contact_path(@lampiran_eleven_close_contact_information,@lampiran_eleven_close_contact_information.specimen_contact), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  </tbody>
  <% end %>
</table>

<br>

