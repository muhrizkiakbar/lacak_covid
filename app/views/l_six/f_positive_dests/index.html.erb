<p id="notice"><%= notice %></p>

<h1>L Six F Positive Dests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Relation</th>
      <th>Date contact</th>
      <th>Last date contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @l_six_f_positive_dests.each do |l_six_f_positive_dest| %>
      <tr>
        <td><%= l_six_f_positive_dest.name %></td>
        
        <td>
        
          <% if current_user.public_health_center.nil? %>
            Disembunyikan.
          <% else %>
            <%= l_six_f_positive_dest.address %>
          <% end %>
        </td>
        <td><%= l_six_f_positive_dest.relation %></td>
        <td><%= l_six_f_positive_dest.date_contact %></td>
        <td><%= l_six_f_positive_dest.last_date_contact %></td>
        <td><%= link_to 'Show', l_six_f_positive_dest %></td>
        <td><%= link_to 'Edit', edit_l_six_f_positive_dest_path(l_six_f_positive_dest) %></td>
        <td><%= link_to 'Destroy', l_six_f_positive_dest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New L Six F Positive Dest', new_l_six_f_positive_dest_path %>
